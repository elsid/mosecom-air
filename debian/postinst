id -u mosecom-air || useradd -rs /bin/false mosecom-air
mkdir -p /var/log/mosecom-air /var/run/mosecom-air
chown -R mosecom-air:mosecom-air /var/log/mosecom-air /var/run/mosecom-air
mkdir -p /var/run/mosecom-air
chown -R mosecom-air:mosecom-air /var/run/mosecom-air
mkdir -p /var/log/nginx/mosecom-air
chown -R www-data:www-data /var/log/nginx/mosecom-air
/etc/init.d/mosecom-air stop
/etc/init.d/mosecom-air start
